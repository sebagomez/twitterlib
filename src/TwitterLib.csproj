<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <AssemblyName>Sebagomez.TwitterLib</AssemblyName>
    <RootNamespace>Sebagomez.TwitterLib</RootNamespace>
    <ApplicationIcon>TwitterLib.ico</ApplicationIcon>
    <Authors>@sebagomez</Authors>
    <Description>(pretty)Complete Twitter Library for the .NET platform</Description>
    <Copyright>Copyright 2022 @SebaGomez</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/sebagomez/twitterlib</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>twitter,twitter-api,oauth</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/sebagomez/twitterlib/master/src/TwitterLib.ico</PackageIconUrl>
    <PackageIcon>TwitterLib.png</PackageIcon>
    <RepositoryUrl>https://github.com/sebagomez/twitterlib</RepositoryUrl>
    <Version>1.7.2</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>- Upgrade to .NET 7</PackageReleaseNotes>
    <ReleaseVersion>0.8</ReleaseVersion>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
    <None Include="TwitterLib.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
